{
  "name": "swpu-hire-fronted",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "reset": "npx rimraf node_modules ./**/node_modules",
    "dev": "modern dev",
    "build": "modern build",
    "start": "modern start",
    "serve": "modern serve",
    "new": "modern new",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "prepare": "simple-git-hooks",
    "upgrade": "modern upgrade"
  },
  "engines": {
    "node": ">=16.18.1"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "@modern-js/runtime": "2.63.3",
    "ahooks": "^3.8.4",
    "antd": "^5.22.6",
    "antd-style": "^3.7.1",
    "axios": "^1.7.9",
    "crypto-js": "^4.2.0",
    "jotai": "^2.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@modern-js/app-tools": "2.63.3",
    "@modern-js/tsconfig": "2.63.3",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "~18.11.9",
    "@types/react": "^18.3.11",
    "@types/react-dom": "~18.3.1",
    "lint-staged": "~13.1.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.0.4"
  }
}
